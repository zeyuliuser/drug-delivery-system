<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 配置映射 -->
<hibernate-mapping package="com.entity">
	<class name="DrugStoreInfo" table="drugstoreinfo">
		<id name="drugstoreid" column="DrugstoreId">
         <generator class="assigned">
         </generator>
		</id>
		<property name="saddress" column="SAddress"></property>
		<property name="drugstore_x" column="DrugStoreX"></property>
		<property name="drugstore_y" column="DrugStoreY"></property>
	</class>
</hibernate-mapping> 