<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <!-- 配置映射 -->
<hibernate-mapping package="com.entity">
	<class name="OrderInfo" table="orderinfo">
		<id name="oid" column="OId">
         <generator class="native"></generator>
		</id>
		<property name="state" column="State"></property>
		<property name="address" column="Address"></property>
		<property name="cid" column="CId"></property>
		<property name="phone" column="Phone"></property>
		<property name="id" column="Id"></property>
		<property name="odesc" column="ODesc"></property>
		<property name="allamount" column="Allamount"></property>
		<property name="staffid" column="Staffid"></property>
		<property name="srname" column="SRName"></property>
		<property name="saddress" column="SAddress"></property>
		<property name="sphone" column="SPhone"></property>
		<property name="lid" column="LId"></property>
		<property name="lrname" column="LRName"></property>
		<property name="logisticaddress" column="Logisticaddress"></property>
		<property name="carid" column="Carid"></property>
		<property name="isrx" column="IsRx"></property>
		<property name="webpic" column="Webpic"></property>

	</class>
</hibernate-mapping> 
